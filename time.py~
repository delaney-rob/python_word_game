# 
# This is the time function I was trying to explain to you earlier
# 
import time

startTime = time.time() # time.time() gives you the seconds elapsed since the EPOCH in floating point format
endTime = time.time()
totalTime = endTime - startTime
print ('\n\nTime : ' + str(round(totalTime, 2))) #round takes floating point num and rounds it, in 
                                                 #this case, to two decimal points. str converts it to string

##################################################################
#This is the shorter version

sTime = time.time()
print ('\n\nTime : ' + str(round(time.time() - sTime, 2)) + '\n\n')
